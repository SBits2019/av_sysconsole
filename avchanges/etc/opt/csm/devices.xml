/etc/opt/csm/devices.xml
-----------------------------
<?xml version="1.0" encoding="utf-8"?>

<CloudTVDevices xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.avnetworks.com/Devices">

  <deviceClasses>

<!-- Default Codec Profile -->

		<configitem name="supportedDisplays" type="string">hd720</configitem>
		<configitem name="videoCodec" type="string">mpeg2</configitem>
		<configitem name="audioCodec" type="string">ac3</configitem>
		<configitem name="abps" type="int">128</configitem>
		<configitem name="achannels" type="int">2</configitem>
		<configitem name="bitrateMode" type="string">vbr</configitem>

<!---<configitem name="deviceBitrate" type="int">NO DEFAULT, use to override bitrate from programs.xml</configitem>-->

<!-- Default Stream Profile -->

		<configitem name="TSProgramID" type="int">1</configitem>
		<configitem name="TSProgramPID" type="int">64</configitem>
		<configitem name="TSVideoPID" type="int">65</configitem>
		<configitem name="TSAudioPID" type="int">66</configitem>
		<configitem name="TSPcrPID" type="int">67</configitem>
		<configitem name="initialIFrames" type="int">5</configitem>
		<configitem name="sessionTsPsiInterval" type="int">1</configitem>

<!-- Default overlays -->

		<configitem name="overlayEncodings" type="string">png</configitem>

		<configitem name="handoffuris" type="string">tv,si,hls,cable,dash,mss,app,url,http+pd</configitem>

<!--
    +++++++++++++++++++++++++++++++++++++++++++++++++++
           Cable Transport (QAM)
    +++++++++++++++++++++++++++++++++++++++++++++++++++
-->

	<deviceClass name="CableTransport">
		<configitem name="deviceProfile" type="string">file:///profiles/qam.xml</configitem>
		<configitem name="supportedDisplays" type="string">hd720,ntsc</configitem>
		<configitem name="transport" type="string">udp</configitem>
		<configitem name="allocateResources" type="bool">true</configitem>
		<configitem name="RMax" type="int">50</configitem>
		<configitem name="initialIFrames" type="int">2</configitem>
		<configitem name="initialIFrameSpacing" type="int">30</configitem>
		<configitem name="intraMbRefreshRate" type="int">125000</configitem>
		<configitem name="intraMbRefreshTimeout" type="int">120</configitem>
		<configitem type="string" name="bufferingStream">0.000</configitem>
<!-- Streaming Profile -->

		<configitem name="bitrateMode" type="string">vbr</configitem>
		<configitem name="TSProgramPID" type="int">480</configitem>
		<configitem name="TSVideoPID" type="int">481</configitem>
		<configitem name="TSAudioPID" type="int">482</configitem>
		<configitem name="TSPcrPID" type="int">481</configitem>
		<deviceClass name="StandardCableClient">
<!-- Default overlays -->

			<configitem name="overlayEncodings" type="string">png</configitem>
			<configitem name="handoffuris" type="string">tv,si,app</configitem>
			
			<!-- <configitem name="sessionDebug" type="string">/var/tmp/%S_debug_%D.ts</configitem> -->
      
		</deviceClass>
      
